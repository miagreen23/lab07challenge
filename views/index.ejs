<!DOCTYPE html>
<html lang="en">
<head>
    <title>Web App</title>
    <link rel="stylesheet" href="mystyles.css">
</head>
<body>
   
    <%- include('header') %>
    
        <main>
        
            <div class="grid-row">
                <% mydata.forEach((toy)=> { %>
            <div class="grid-item">
                <%= toy.title.slice(0, 30)%>
                
                <div> 
                    <button><a href="<%= toy.url %>">BUY</a></button>
                </div>
            </div>
                <% }) %>
            </div>
            
        </main>
    
<%- include('footer') %>

</body>
</html>